// xtime header for threads library
#pragma once
#ifndef _THR_XTIME
#define _THR_XTIME
#ifndef RC_INVOKED
#include <thr/xtimec.h>

 #pragma pack(push,_CRT_PACKING)
 #pragma warning(push,3)
 #pragma push_macro("new")
 #undef new

namespace stdext
	{	// Dinkum Libraries
	namespace threads
		{	// Dinkum C++ Threads Library
using ::xtime;
using ::TIME_UTC;
using ::xtime_get;
		}	// namespace threads
	}	// namespace stdext
 #pragma pop_macro("new")
 #pragma warning(pop)
 #pragma pack(pop)
#endif /* RC_INVOKED */
#endif /* _THR_XTIME */

