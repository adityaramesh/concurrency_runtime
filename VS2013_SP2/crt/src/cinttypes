#pragma once
#ifndef _CINTTYPES_
#define _CINTTYPES_
#include <yvals.h>

#ifdef _STD_USING
 #undef _STD_USING
  #include <inttypes.h>
 #define _STD_USING

#else /* _STD_USING */
 #include <inttypes.h>
#endif /* _STD_USING */

#include <cstdint>

 #if _GLOBAL_USING && !defined(RC_INVOKED)
_STD_BEGIN
using _CSTD imaxdiv_t;	// from <inttypes.h>

using _CSTD imaxabs; using _CSTD imaxdiv;
using _CSTD strtoimax; using _CSTD strtoumax;
using _CSTD wcstoimax; using _CSTD wcstoumax;
_STD_END
 #endif /* _GLOBAL_USING */

#endif /* _CINTTYPES_ */

